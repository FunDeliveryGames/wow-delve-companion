<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://raw.githubusercontent.com/Gethe/wow-ui-source/live/Interface/AddOns/Blizzard_SharedXML/UI.xsd">
    <Frame name="DelvelCompanionDelveOBotWidgetTemplate"
        inherits="DelveCompanionCustomActionWidgetTemplate"
        mixin="DelveCompanion_DelveOBotWidgetMixin" virtual="true">
        <Layers>
            <Layer level="OVERLAY" textureSubLevel="5">
                <Texture parentKey="SlotFrameCollected"
                    atlas="collections-itemborder-collected">
                    <Anchors>
                        <Anchor point="CENTER" x="0" />
                    </Anchors>
                </Texture>
                <Texture parentKey="SlotFrameUncollected"
                    atlas="collections-itemborder-uncollected" hidden="true"
                    useAtlasSize="false">
                    <Anchors>
                        <Anchor point="CENTER" x="0" y="2" />
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad method="OnLoad" />
            <OnShow method="OnShow" />
            <OnHide method="OnHide" />
            <OnEvent method="OnEvent" />
        </Scripts>
    </Frame>
</Ui>